    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug CSharpGenerator",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build CSharpGenerator",
            "program": "${workspaceFolder}/CSharpGenerator/bin/Debug/net5.0/FluentKusto.CSharpGenerator.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole",
            "env": {
                "WriteSchemaToFiles": "true",
                "GenerateCSharpFromFiles": "true"
            }
        },
        {
            "name": "Debug TestClient",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build TestClient",
            "program": "${workspaceFolder}/TestClient/bin/Debug/net5.0/FluentKusto.TestClient.dll",
            "args": [],
            "cwd": "${workspaceFolder}",
            "stopAtEntry": false,
            "console": "internalConsole",
            "env": {
                "WriteSchemaToFiles": "true",
                "GenerateCSharpFromFiles": "false"
            }
        }


    ]
}