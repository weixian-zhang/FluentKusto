@using FluentKusto.CSharpGenerator

using System;
using System.Linq.Expressions;

namespace FluentKusto
{
    public class @Model.Name : TableBase<@Model.Name>
    {
        @foreach (var column in @Model.Columns)
        {
            @if(@column.Type == "datetime")
            {
            <text>
            /// @column.Description
            public DateTime @column.Name {get; set;}
            </text>
            continue;
            }

            @if(@column.Type == "real")
            {
            <text>
            /// @column.Description
            public float @column.Name {get; set;}
            </text>
            continue;
            }

            <text>
            /// @column.Description
            public @column.Type @column.Name {get; set;}
            </text>
        }
    }
}